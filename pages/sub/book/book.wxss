Page{
  width: 100%;
  background: #f4f4f7;
}
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.info{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.items{
  display: flex;
  flex-direction: column;
  padding: 15rpx auto;
  text-align: center;
}
.names{
  color: #a1a1a1;
  font-size: 32rpx;
  font-weight: 600;
}
.values{
  color: #000;
  font-size: 32rpx;
  margin: 15rpx auto;
}
.title{
  margin-top: 35rpx;
  font-size: 32rpx;
  color: #a1a1a1;
  margin-left: 20rpx;
}
.list{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  margin: 20rpx 0;
}
.item{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.name{
  color: #a1a1a1;
  font-size: 32rpx;
  margin: 20rpx;
  white-space: nowrap;
}
.value{
  color: #000;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 20rpx 0;
}
.value-tip{
  color: crimson;
  font-size: 32rpx;
  margin: 20rpx 0;
}
.line{
  width: 100%;
  height: 3rpx;
  background: #bdbdbd;
}
.empty{
  width: 100%;
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-top: 340rpx;
}
.none{
  width: 150rpx;
  height: 150rpx;
  margin: 30rpx auto;
}
.none-text{
  font-size: 32rpx;
  color: #000;
  margin: 0 auto;
}