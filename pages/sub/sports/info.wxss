Page{
  width: 100%;
  background: #f4f4f7;
}
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.top{
  width: 100%;
  display: flex;
  flex-direction: row;
  background: #fff;
  padding: 25rpx 0;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.body{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  margin: 30rpx 0;
}
.list{
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 20rpx 0;
  justify-content: space-between;
}
.title{
  color: #3f3f3f;
  font-size: 32rpx;
  margin-left: 20rpx;
}
.score{
  color: #000;
  font-size: 32rpx;
  margin-right: 28rpx;
}
.line{
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
}
/* 底部版权 */
.copyright{
  width: 100%;
  display: flex;
  text-align: center;
  margin: 15rpx auto;
}
.copyright text{
  color: #a1a1a1;
  font-size: 22rpx;
  margin: 0 auto;
}