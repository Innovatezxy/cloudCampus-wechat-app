Page{
  width: 100%;
  background: #fff;
}
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.logo{
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 30rpx auto 0;
}
.logo-img{
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}
.logo-text-a{
  font-size: 38rpx;
  color: #000;
  margin: 10rpx auto;
}
.logo-text-b{
  font-size: 28rpx;
  color: #666666;
  margin-bottom: 60rpx;
}
.line{
  width: 100%;
  height: 3rpx;
  background: #a1a1a1;
}
.status{
  color: #048280;
  font-size: 32rpx;
  padding: 20rpx 35rpx;
}
.body{
  width: 100%;
  text-align: center;
}
.title_1{
  color: #707070;
  font-size: 32rpx;
  margin-left: 35rpx;
  margin-top: 20rpx;
}
.title_2{
  color: #707070;
  font-size: 32rpx;
  margin: 10rpx 35rpx;
}
.items{
  display: flex;
  width: 90%;
  height: 90rpx;
  align-items: center;
  margin: 30rpx auto;
  border-radius: 30rpx;
  box-shadow: 0 0 5rpx 5rpx rgba(150, 150, 150, 0.233);
}
.items text{
  color: #4e4e4e;
  font-size: 36rpx;
  margin: auto;
}
.tips{
  color: #a1a1a1;
  font-size: 25rpx;
  margin: 30rpx auto;
}
.copyright{
  font-size: 22rpx;
  color: #a1a1a1;
  text-align: center;
  width: 100%;
  margin-top: 30rpx;
}